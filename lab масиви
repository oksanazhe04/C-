using System;

class Program
{
    static void Main()
    {
        int[] numbers = { 2, 45, 23, 12, 7};

        int sum = 0;
        foreach (int number in numbers)
        {
            sum += number;
        }

        double average = (double)sum / numbers.Length;

        Console.WriteLine("Середнє арифметичне: " + average);


    
    }
}
